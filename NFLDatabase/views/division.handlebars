      <!-- Add Division Form-->
      <!--===================-->
      <div class="content-container" style="margin: 20px 30%;">
        <div class="col-lg-12 text-left"> 
            <legend> Add Division</legend> 
            <form action="/division" method="POST">
                <label>Name: <input type="text" name="name" placeholder="name" required> </label>
               <p></p>
               <p>
                 <button class="add" type="submit"><i class="fa fa-plus" aria-hidden="true"></i>Add Division</button>
                </p>
            </form>
          </div>
        </div>
        
        <!--=================-->
        <!-- Division Table  -->
        <!--=================-->
        <div class="content-container" style="justify-content: center;"> 
          <div class="container container-fluid col-lg-9 scrollTable">   
              <table class= "table table-bordered">
                <caption>Divisions Table</caption>
                <thead> 
                    <th>Name </th>
                </thead> 
                <tbody> 
                    {{#each division}} 
                    <tr> 
                        <td>{{name}}</td>
                        <td><button class="delete" onclick="deleteDivision({{id}})"><i class="fa fa-trash-o" aria-hidden="true"></i>Delete</button></td>
                    
                    <form action="/updateDivision" method="GET">
                          <input type="hidden" name="id" value = "{{this.id}}">
                          <td><button type="submit" class="update">Update</a></button></td>
                    </form>
                    </tr> 
                    {{/each}} 
                  </tbody>
                </table>
              </div>
          </div>
